<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="EVENT_DEF">
	<select id="listAll" resultType="com.smartool.common.dto.EventDef">
		select * from event_def;
	</select>
	<select id="listBySerieId" resultType="com.smartool.common.dto.EventDef" parameterType="String">
		select * from event_def where eventSerieDefId in (select eventSerieDefId from series where id=#{serieId})
	</select>
</mapper>